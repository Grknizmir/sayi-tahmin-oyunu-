    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="sayi, n" type="Integer" array="False" size=""/>
            <assign variable="sayi" expression="0"/>
            <assign variable="n" expression="1"/>
            <output expression="&quot;sayi giriniz&quot;"/>
            <input variable="sayi"/>
            <if expression="sayi&gt;0">
                <then>
                    <while expression="n*(n+1)/2!=sayi&amp;&amp;sayi&gt;n">
                        <assign variable="n" expression="n+1"/>
                    </while>
                </then>
                <else>
                    <while expression="sayi&lt;0">
                        <output expression="&quot;sayi negatif l&#252;tfen pozitif bir say&#305; giriniz&quot;"/>
