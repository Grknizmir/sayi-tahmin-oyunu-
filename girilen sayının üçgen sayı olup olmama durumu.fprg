                    </while>
                    <while expression="n*(n+1)/2!=sayi&amp;&amp;sayi&gt;n">
                        <assign variable="n" expression="n+1"/>
                    </while>
                </else>
            </if>
            <if expression="n*(n+1)/2==sayi&amp;&amp;sayi&gt;n">
                <then>
                    <output expression="&quot;sayiniz &#252;&#231;gen sayidir&quot;"/>
                </then>
                <else>
                    <output expression="&quot;Say&#305;n&#305;z &#252;&#231;gen say&#305; degildir&quot;"/>
